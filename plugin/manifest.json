{
  "name": "Canvas NCBS Dark mode",
  "version": "1.1.0",
  "manifest_version": 2,
  "description": "Dark Mode for Canvas NBCS.",
  "homepage_url": "https://canvas.nbcs.nsw.edu.au",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
      "https://canvas.nbcs.nsw.edu.au/*",
      "https://*.instructure.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://canvas.nbcs.nsw.edu.au/*",
        "https://*.instructure.com/*"
      ],
      "css":[
          "css/styles.css"
      ]
    }
  ]
}
